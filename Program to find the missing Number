#include<iostream>
using namespace std;
int main()
{
	int i, n, arr[20];
	cout << "enter size of array: ";
	cin >> n;
	cout << "enter array elements from 1 to " << n + 1 << endl;
	for (i = 1; i < n+1; i++)
	{
		cin >> arr[i];
	}
	cout << "The missing element from 1 to " << n + 1 << " is: ";
	for (i = 1;i <= n+1; i++)
	{
		if (arr[i] == i)
			continue;
		else
		{
			cout << i;
			break;
		}
	}

}
